{
  "version": 2,
  "name": "pilapeback-api",
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "env": {
    "VERCEL": "1",
    "FLASK_ENV": "production",
    "DEBUG": "False"
  },
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api.py"
    }
  ]
}